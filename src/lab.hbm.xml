<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.beans.lab" table="LAB">
		<meta attribute="class-description">
			This class contains the Laboratory details.
		</meta>
		<id name="labId" type="string" column="labId">
			<generator class="native" />
		</id>
		<property name="labName" column="LAB_NAME" type="string" />
		<property name="branchCode" column="LAB_BRANCH_CODE" type="string" />
		<property name="labRepresentative" column="LAB_REP" type="string" />
		<property name="labOwner" column="LAB_OWNER" type="string" />
		<property name="status" column="STATUS" type="string" />
		<property name="primaryMobileNo" column="PRIM_MOBILE"
			type="string" />
		<property name="secondaryMobileNo" column="SECOND_MOBILE"
			type="string" />

	</class>
</hibernate-mapping>